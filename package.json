{
  "name": "style-properties",
  "version": "1.1.0",
  "description": "JavaScript library for getting information about element style properties in unified and usable format.",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/fczbkk/style-properties.git"
  },
  "author": {
    "name": "Riki Fridrich",
    "email": "riki@fczbkk.com",
    "url": "http://fczbkk.com/"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fczbkk/style-properties/issues"
  },
  "homepage": "https://github.com/fczbkk/style-properties#readme",
  "scripts": {
    "cleanup": "rimraf temp lib",
    "prebuild": "npm run test && npm run cleanup",
    "build": "babel src -d lib",
    "test": "karma start ./karma.conf.js",
    "dev": "npm run test -- --no-single-run --auto-watch",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "postversion": "git push && git push --tags && npm publish",
    "version": "npm run build && npm run changelog && git add -A",
    "doc": "documentation readme ./src/index.js -s 'Documentation'"
  },
  "main": "lib/index.js",
  "devDependencies": {
    "babel-cli": "^6.6.5",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "conventional-changelog-cli": "^1.1.1",
    "documentation": "^4.0.0-beta2",
    "jasmine-core": "^2.4.1",
    "karma": "^0.13.22",
    "karma-coverage": "^0.5.5",
    "karma-jasmine": "^0.3.8",
    "karma-mocha-reporter": "^2.0.0",
    "karma-phantomjs-launcher": "^1.0.0",
    "karma-webpack": "^1.7.0",
    "mocha": "^2.4.5",
    "phantomjs-prebuilt": "^2.1.7",
    "rimraf": "^2.5.2",
    "webpack": "^1.12.14"
  }
}
